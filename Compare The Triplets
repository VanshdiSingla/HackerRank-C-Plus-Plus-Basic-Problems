// https://www.hackerrank.com/challenges/compare-the-triplets/problem?isFullScreen=true

#include<iostream>
using namespace std;
int main()
{
    int a[3];
    int b[3];
    for(int i=0;i<3;i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<3;i++)
    {
        cin>>b[i];
    }
    int counta=0;
    int countb=0;
    for(int i=0;i<3;i++)
    {
        if(a[i]>b[i])
        {
            counta++;
        }
        else if(a[i]<b[i])
        {
            countb++;
        }
    }
    cout<<counta<<" "<<countb<<endl;
}
